{
	"version": 2,

    "layout": {
        "contentSize": "640px",
        "wideSize": "940px"
    },
    "settings": {
        "typography": {
            "fontFamilies": [
                {
                    "fontFace": [
                        {
                            "fontFamily": "Merriweather",
                            "fontWeight": "400",
                            "src": [
                                "file:./assets/merriweather/merriweather-v30-latin-regular.woff2"
                            ]
                        }
                    ],
                    "fontFamily": "\"Merriweather\", serif",
                    "name": "Merriweather",
                    "slug": "merriweather"
                }
    
            ]
        }
    },
    "styles": {
		"typography": {
			"fontSize": "var(--wp--preset--font-size--normal)",
			"fontFamily": "var(--wp--preset--font-family--merriweather)"
		},
        "blocks": {
            "core/site-title": {
                "elements": {
                    "link": {
                        "color": {
                            "text": "white"
                        },
                        "typography": {
                            "textDecoration": "none"
                        }
                    }
                },
                "spacing": {
                    "margin": "0px"
                }
            },
            "core/site-tagline": {
                "spacing": {
                    "margin": "0px"
                }
            },
			"core/navigation": {
				"elements": {
					"link": {
                        "color": {
                            "text": "white"
                        },
                        "spacing": {
                            "padding": {
                                "right": "16px"
                            }
                        },
						":hover": {
                            "color": {
                                "text": "red"
                            },
							"typography": {
								"textDecoration": "underline"
							}
						},
						":focus": {
							"typography": {
								"textDecoration": "underline dashed"
							}
						},
						":active": {
							"typography": {
								"textDecoration": "none"
							}
						},
						"typography": {
							"textDecoration": "none"
						}
					}
				}
			}
        }
    }
}
